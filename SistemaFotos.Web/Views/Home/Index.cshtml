@model List<SistemaFotos.Web.Models.Imagem>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <div class="container">
        <div class="row">
            @foreach (var img in Model)
            {
                <div class="col-sm">
                    <img class="rounded" src="@(img.Caminho)">
                    <p>@img.Titulo</p>
                    <br />
                    <br />
                </div>
            }
        </div>
    </div>
</div>
